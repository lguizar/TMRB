<div class="container" id="Index">
  <div class="jumbotron">
      <h1 align=center style="font-family:verdana;">Good evening, [NAME]!</h1>
    </div>


	<div class="container" id="New Location">
    <!-- h1 through h6 are headers. The higher the number, the smaller the header -->
    <!-- p means paragraph -->
		<h3>Schedule</h3>
		{{#each events}}
				<div class="events">

					<h6><b><li>{{description}}</li></b></h6>
					<hr />
				</div>
			{{/each}}

		<!--
		-->
    {{#each locations}}
        <div class="location">

          <h3>{{name}}</h3>
          <h6><b>{{description}}</b></h6>
          <hr />
        </div>
      {{/each}}

