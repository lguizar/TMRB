<html>
<head>
	<title>Skeleton Schedule Page</title>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">

</head>
<body>
	<link href="/css/bootstrap.min.css" rel="stylesheet">
	<link href="/css/bootstrap-theme.min.css" rel="stylesheet">
	<link href="/css/introHCI.css" rel="stylesheet">

	<div class="container" id="New Event">
    <!-- h1 through h6 are headers. The higher the number, the smaller the header -->
    <!-- p means paragraph -->
    <div class="jumbotron">
      <h1 align=center>Edit Morning Recap</h1>
    </div>

		<div class="page" id="page2">
			<a href="/index">
				<p align="center">Confirm</p>
			</a>
		</div>

    <div class="page" id="page1">
      <a href="/index">
        <p align = "center">Back</p>
      </a>
    </div>

    <form id="addEventForm" role="form" method="get" action="/addto">
      <h3>Add an event</h3>
        <div class="form-group">
          <label for="name">Event Name:</label>
          <input type="text" class="form-control" id="name" placeholder="Name" name="name">

          <label for="time">Time:</label>
          <input type="text" class="form-control" id="time" placeholder="Time" name="time">

          <label for="location">Location:</label>
          <input type="text" class="form-control" id="location" placeholder="Location" name="location">
        </div>
        <input type="submit" id="submitBtn" class="btn btn-default" value="Add the event"></input>
    </form>






<script src="https://code.jquery.com/jquery.js"></script>
<script src="/js/bootstrap.min.js"></script>
<script src="/js/newEvent.js"></script>

</body>
</html>
